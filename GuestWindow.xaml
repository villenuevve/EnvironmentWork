<Window x:Class="EnvironmentWork.EventLogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Event Log" Height="650" Width="1000" MaxWidth="1000">
    <Grid>
        <DataGrid x:Name="eventLogGrid" AutoGenerateColumns="True" />
        <Button Content="Analysis" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,100,10"
                 Width="75" Height="30" Click="AnalysisButton_Click"/>
        <Button Content="Exit" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10"
                Width="75" Height="30" Click="ExitButton_Click" />
    </Grid>
</Window>


